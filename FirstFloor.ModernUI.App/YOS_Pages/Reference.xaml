<UserControl x:Class="FirstFloor.ModernUI.App.YOS_Pages.Reference"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/YOS_Pages/Reference_Pages/Patners.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Partners" Source="/YOS_Pages/Reference_Pages/Patners.xaml" />
                <mui:Link DisplayName="항목별 요율" Source="/YOS_Pages/Reference_Pages/ItemRate.xaml" />
                <mui:Link DisplayName="강좌 분류" Source="/YOS_Pages/Reference_Pages/CourseCategory.xaml" />
                <mui:Link DisplayName="교구 단가" Source="/YOS_Pages/Reference_Pages/TeachingMaterialPrice.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>

</UserControl>
