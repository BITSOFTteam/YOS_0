             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
            <mui:ModernTab SelectedSource="/YOS_Pages/Course_Pages/Courses/Whole.xaml" Layout="Tab">
                <mui:ModernTab.Links>
                    <mui:Link DisplayName="전체" Source="/YOS_Pages/Course_Pages/Courses/Whole.xaml" />
                    <mui:Link DisplayName="파트너 필터" Source="/YOS_Pages/Course_Pages/Courses/Patners.xaml" />
                <mui:Link DisplayName="역할 필터" Source="/YOS_Pages/Course_Pages/Courses/Role.xaml"/>
            </mui:ModernTab.Links>
            </mui:ModernTab>
    </Grid>
</UserControl>
